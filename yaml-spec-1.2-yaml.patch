--- yaml-spec-1.2.yaml	2020-10-15 14:40:06.172498651 -0400
+++ yaml-spec-1.2-patch.yaml	2020-10-15 15:07:19.617994006 -0400
@@ -2553,15 +2553,15 @@
 #   s-b-comment
 
 c-b-block-header:
-  (...): [ m, t ]
+  (...): n
   (all):
   - (any):
     - (all):
-      - c-indentation-indicator: m
-      - c-chomping-indicator: t
+      - c-indentation-indicator: n
+      - c-chomping-indicator
     - (all):
-      - c-chomping-indicator: t
-      - c-indentation-indicator: m
+      - c-chomping-indicator
+      - c-indentation-indicator: n
   - s-b-comment
 
 
@@ -2584,12 +2584,12 @@
 #   ( <empty> => m = auto-detect() )
 
 c-indentation-indicator:
-  (...): m
+  (...): n
   (any):
   - (if): ns-dec-digit
     (set): [ m, { (ord): (match) } ]
   - (if): <empty>
-    (set): [ m, "auto-detect" ]
+    (set): [ m, { auto-detect: n } ]
 
 
 
@@ -2612,7 +2612,6 @@
 #   ( <empty> => t = clip )
 
 c-chomping-indicator:
-  (...): t
   (any):
   - (if): '-'
     (set): [ t, "strip" ]
@@ -2722,7 +2721,7 @@
   (...): n
   (all):
   - '|'
-  - c-b-block-header: [ m, t ]
+  - c-b-block-header: n
   - l-literal-content: [ { (+): [ n, m ] }, t ]
 
 
@@ -2795,7 +2794,7 @@
   (...): n
   (all):
   - '>'
-  - c-b-block-header: [ m, t ]
+  - c-b-block-header: n
   - l-folded-content: [ { (+): [ n, m ] }, t ]
 
 
@@ -3409,18 +3408,19 @@
